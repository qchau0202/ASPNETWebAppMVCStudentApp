        </Field>
        <Field Name="Grade">
          <DataField>Grade</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>SchoolDB</DataSourceName>
        <CommandText>
          SELECT s.StudentID, s.FirstName, s.LastName, e.Grade
          FROM Students s
          JOIN Enrollments e ON s.StudentID = e.StudentID
          WHERE e.CourseID = @CourseID
        </CommandText>
      </Query>
    </DataSet>
  </DataSets>
